iptables -F

iptables -A INPUT -d 93.101.30.55 -m connlimit --connlimit-above 7 -j REJECT 
iptables -A OUTPUT -s 93.101.30.55 -m connlimit --connlimit-above 7 -j REJECT 

iptables -A INPUT -d 93.101.30.55 -m state --state ESTABLISHED,RELATED -j ACCEPT
iptables -A OUTPUT -s 93.101.30.55 -m state --state ESTABLISHED,RELATED -j ACCEPT
